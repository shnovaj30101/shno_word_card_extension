{
    "manifest_version": 3,
    "name": "Anki單字卡擷取介面",
    "description": "簡便的增加anki單字卡",
    "version": "1.0.2",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "action": {
        "default_icon": "img/icon.png",
        "default_popup": "bg/popup.html"
    },
    "background": {
        "service_worker" : "bg/background.js",
        "type": "module"
    },
	"content_scripts" : [
		{
			"matches" : ["*://*/*"],
			"js" : ["lib/jquery.min.js", "fg/content.js"]
		}
	],
    "permissions": []
}
